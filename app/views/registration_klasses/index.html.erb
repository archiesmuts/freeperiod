<p id="notice"><%= notice %></p>

<h1>Registration Klasses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>School</th>
      <th>Course</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_klasses.each do |registration_klass| %>
      <tr>
        <td><%= registration_klass.name %></td>
        <td><%= registration_klass.school_id %></td>
        <td><%= registration_klass.course_id %></td>
        <td><%= registration_klass.year %></td>
        <td><%= link_to 'Show', registration_klass %></td>
        <td><%= link_to 'Edit', edit_registration_klass_path(registration_klass) %></td>
        <td><%= link_to 'Destroy', registration_klass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration Klass', new_registration_klass_path %>
