<p id="notice"><%= notice %></p>

<h1>User Registrations</h1>

<table>
  <thead>
    <tr>
      <th>Registration klass</th>
      <th>User type</th>
      <th>Completed</th>
      <th>Completed at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_registrations.each do |user_registration| %>
      <tr>
        <td><%= user_registration.registration_klass_id %></td>
        <td><%= user_registration.user_type %></td>
        <td><%= user_registration.completed %></td>
        <td><%= user_registration.completed_at %></td>
        <td><%= link_to 'Show', user_registration %></td>
        <td><%= link_to 'Edit', edit_user_registration_path(user_registration) %></td>
        <td><%= link_to 'Destroy', user_registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Registration', new_user_registration_path %>
