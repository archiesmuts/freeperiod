<% if @user_registration.user.has_role? "educator",  @user_registration.school %>
  <% if @user_registration.registration_klass.course_name.downcase.include? "sport" %>
    Coaching staff
    <% else %>
    Teacher
  <% end %>
<% else %>
  <% if @user_registration.registration_klass.course_name.downcase.include? "sport" %>
    Player
    <% else %>
    Learner
  <% end %>
<% end %>
