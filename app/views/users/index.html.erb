<div class="container">
    <h3>Users</h3>
      <table class="table">
        <thead>
          <th>
            Name
          </th>
          <th>
            School
          </th>
          <th>
            Roles
          </th>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
</div>
